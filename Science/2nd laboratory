import math

x = 16.55 * 10**(-3)
y = -2.75
z = 0.15

s_0 = x ** (1/3)
s_05 = x ** (y + 2)
s_1 = math.sqrt(10 * (s_0 + s_05))
s_15 = abs(x - y)
s_2 = (math.asin(z) ** 2)

s = s_1 * (s_2 - s_15)
