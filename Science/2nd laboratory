import math
x = -15.246
y = 4.642*10**(-2)
z = 21
f_0 = (x-(y/2))
f_1 = (math.sin(math.atg(z))**2)
f_15 = -(math.sqrt(abs(x)))
f_2 = math.ln(y**(f_15))
