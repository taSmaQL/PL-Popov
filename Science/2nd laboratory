import math
x = 17.421
y = 10.365*10**(-3)
z = 0.828*10**5 

ksuha_0 = math.pow(y+(math.pow(x-1,1/3),1/4))
ksuha_05 = abs(x-y)
ksuha_1 = (math.sin(z)**2+math.tan(z))
ksuha_answer = ((ksuha_0)/(ksuha_05*ksuha_1))
# 14
import math
x = 12.3 * 10**(-1)
y = 15.4 
z = 0.252*10**3

shatilov_0 = y**(x+1)
shatilov_05 = math.pow(abs(y-2),1/3) + 3
shatilov_1 = (x+(y/2))
shatilov_15 = 2*abs(x+y)
shatilov_2 = (x+1)**((-1)/(math.sin(z)))
shatilov_answer = shatilov_0/shatilov_05 + shatilov_1 / shatilov_15 * shatilov_2
# 15
import math
x = 2.444
y = 0.869 * 10**(-2)
z = -0.13 * 10**3

EGE_PO_BORBE_0 = (x**(y+1) + math.exp(y-1))
EGE_PO_BORBE_05 = 1 + x*abs(y - math.tan(z))
EGE_PO_BORBE_1 = (1 + abs(y - x))
EGE_PO_BORBE_15 = (abs(y-x)**2/2)
EGE_PO_BORBE_2 = (abs(y-x)**3/3)
EGE_PO_BORBE_ANSWER = EGE_PO_BORBE_0/EGE_PO_BORBE_05 * EGE_PO_BORBE_1 + EGE_PO_BORBE_15 - EGE_PO_BORBE_2
