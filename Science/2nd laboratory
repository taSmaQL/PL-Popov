import math
x = 0.1722
y = 6.33
z = 3.25*10**(-4)
s_0 = (5*(math.atan(x)))
s_05 = ((1/4)*(math.acos(x)))
s_1 = (x+3*(abs(x-y))+x**2)
s_15 = (abs(x-y)*z + x**2)
s = s_0 - s_05*((s_1)/(s_15))
