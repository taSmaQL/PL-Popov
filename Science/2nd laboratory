import math
x = 3.251
y = 0.325
z = 0.466 * 10**(-4)

nastya_0 = 2**(y**(x))
nastya_05 = ((3**x)**y)
nastya_1 = y*(math.atan(z)-(1/3))
nastya_15 = (abs(x)+((1)/(y**2+1)))
nastya_answer = nastya_0 + nastya_05 - ((nastya_1)/(nastya_15))
