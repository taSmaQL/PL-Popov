import math
x = 6.251
y = 0.827
z = 25.001
python_0 = y**(math.pow((abs(x))),1/3)
python_05 = (math.cos(y)**3)
python_1 = abs(x-y)
python_15 = (1 + ((math.sin(z)**2)/math.sqrt(x+y)))
python_2 = math.exp(abs(x-y)) + (x/2)
python_answer = python_0 + python_05 * ((python_1*python_15)/(python_2))
