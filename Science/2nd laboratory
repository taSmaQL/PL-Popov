import math

x = 0.4 * 10**4
y = -0.875
z = -0.475 * 10**(-3)


p_0 = abs(math.cos(x) - math.cos(y))
p_1 = (1 + 2 * (math.sin(y)**2))
p_2 = (1 + z + (z**2 / 2) + (z**3 / 3) + (z**4 / 4))

p = p_0 ** p_1 * p_2
