import math
x = 1.825 * 10**2 
y = 18.225
z = -3.298*10**(-2)
kirill_0 = abs((x**(y/x)-math.pow((y/x),1/3)))
kirill_05 = (y-x)
kirill_1 = (math.cos(y)-((z)/(y-x)))
kirill_15 = (1+(y-x)**2)
kirill_answer = kirill_0+kirill_05*(kirill_1/kirill_15)
