// 1
def is_ending(str1, str2):
    return str1.endswith(str2)


// 2
def format_update(name):
    format_name = name.capitalize()
    return f"Приветствую, {format_name}!"


// 3
def remove_vow(text):
    vow = "ANY EXAMPLE WHICH YOU WANT TO INTRODUCE"
    return "".join([char for char in text if char not in vow])
// Here is vow depicts vowels***


// 4
def print_squares(start, step):
    current = min(start, step)
    max_num = max(start, step)
    squares = []
    while current <= max_num:
        squares.append(str(current ** 2))
        current += step
    print(", ".join(squares))


// 5
def generate_sequence(start, step, count):
    seq = [str(start + step * i) for i in range(count)]
    print(", ".join(seq))



// 6
memory = []

def update_memory(num):
    if len(memory) < 3:
        memory.append(num)
    else:
        min_index = memory.index(min(memory))
        memory[min_index] = num
    print(memory)

// 7
def remove_duplicates(text):
    words = text.split()
    unique_words = []
    for word in words:
        if word not in unique_words:
            unique_words.append(word)
    return " ".join(unique_words)
